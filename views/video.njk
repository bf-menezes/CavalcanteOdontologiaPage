
{% extends "layout.njk" %}


{% block head %}

<title>{{item.autor}}</title>

{% endblock %}


{% block content %}

<div id = "wrapper">
    <h2>{{item.autor}}</h2>
</div>

<section class="video-window">

    <div class="video" id="{{item.id}}">
        <div class="video_container">
            <iframe src="https://instagram." frameborder="0"></iframe>
        </div>
        <div class="autor">
            <p>{{item.autor}}</p>
        </div>          
    </div>
    
</section>

{% endblock %}



